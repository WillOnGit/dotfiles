[user]
	name = Will Bolton
	email = wilbol@ceh.ac.uk
[core]
	editor = vim
	excludesFile = ~/.git-excludesFile
	pager = less -F
[pull]
	ff = true
[push]
	autoSetupRemote = true
[init]
	defaultBranch = main
[alias]
	fl = log --oneline --graph --branches
	cv = commit --verbose
	mc = log --branches --author=wilbol*
	s = status
	d = diff
	dw = diff --color-words
	dc = diff --cached
	dcw = diff --cached --color-words
